{
  "name": "@anysiteio/mcp",
  "version": "0.6.0",
  "description": "A Model Context Protocol (MCP) server that provides access to AnySite's LinkedIn and Instagram APIs",
  "private": false,
  "type": "module",
  "module": "src/smithery.ts",
  "bin": {
    "anysite-mcp": "build/index.js",
    "anysite": "build/index.js",
    "mcp": "build/index.js"
  },
  "files": [
    "build",
    "src",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "npx smithery build",
    "dev": "npx smithery dev",
    "build:tsc": "tsc && chmod +x build/index.js",
    "prepare": "npm run build:tsc",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@smithery/sdk": "^1.6.8",
    "dotenv": "^16.4.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@smithery/cli": "^1.4.7",
    "@types/node": "^20.17.23",
    "typescript": "^5.8.2"
  },
  "keywords": [
    "mcp",
    "claude",
    "linkedin",
    "instagram",
    "anysite"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/horizondatawave/hdw-mcp-server.git"
  },
  "homepage": "https://anysite.io",
  "author": "AnySite"
}
